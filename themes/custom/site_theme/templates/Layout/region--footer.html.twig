<div class="container footer-header">
  {% for content_section in block_content.section_content %}
    {% if content_section.media_component %}
      <div class="container-fluid p-2 mx-auto d-block pt-4 footer-logo">
        <img src="{{ content_section.media_component.details.uri}}" class="card-img-top" alt="{{ content_section.details.media_component.alt}}">
      </div>
  {% elseif content_section.column_items.text_field %}
      <div class="container-fluid position-relative top-50 start-50 translate-middle-x text-center">
        {{content_section.column_items.text_field|raw}}
      </div>
    {% endif %}
  {% endfor %}
  {{block_content.copyright|raw}}
</div>
